<% if @update.errors.any? %>
  <% @update.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>

<%= form_for @update do |t| %>

<%= t.label :status %>
<%= t.text_field :status %>
<br>
<%= t.label :user %>
<%= t.text_field :user %>
<br>
<%= t.label :likes %>
<%= t.number_field :likes %>
<br>
<%= t.submit %>

<% end %>
