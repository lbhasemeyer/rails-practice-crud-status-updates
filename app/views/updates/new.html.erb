<% if @update.errors.any? %>
  <% @update.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>

<%= form_for @update do |f| %>

<%= f.label :status %>
<%= f.text_field :status %>
<br>
<%= f.label :user %>
<%= f.text_field :user %>
<br>
<%= f.label :likes %>
<%= f.number_field :likes %>
<br>
<%= f.submit %>

<% end %>
